    "@types/react-modal-image": "^2.6.3",
    "css-loader": "^7.1.2",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
