  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^8.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.3.0",
    "react-modal": "^3.16.1",
    "react-modal-image": "^2.6.0",
    "react-router-dom": "^6.11.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
